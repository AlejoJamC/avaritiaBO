doctype html
html.no-js(lang='en')
  // <![endif]
  // BEGIN HEAD
  head
    meta(charset='utf-8')
    title !{ title }
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(content='width=device-width, initial-scale=1', name='viewport')
    meta(content='', name='description')
    meta(content='', name='author')
    // BEGIN GLOBAL MANDATORY STYLES
    link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700&subset=all', rel='stylesheet', type='text/css')
    link(href= level + 'plugins/font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    link(href= level + 'plugins/simple-line-icons/simple-line-icons.min.css', rel='stylesheet', type='text/css')
    link(href= level + 'plugins/bootstrap/css/bootstrap.min.css', rel='stylesheet', type='text/css')
    link(href= level + 'plugins/uniform/css/uniform.default.css', rel='stylesheet', type='text/css')
    link(href= level + 'plugins/bootstrap-switch/css/bootstrap-switch.min.css', rel='stylesheet', type='text/css')
    // END GLOBAL MANDATORY STYLES
    // BEGIN PAGE LEVEL PLUGIN STYLES
    link(href= level + 'plugins/bootstrap-daterangepicker/daterangepicker-bs3.css', rel='stylesheet', type='text/css')
    link(href= level + 'plugins/fullcalendar/fullcalendar.min.css', rel='stylesheet', type='text/css')
    link(href= level + 'plugins/jqvmap/jqvmap/jqvmap.css', rel='stylesheet', type='text/css')
    // END PAGE LEVEL PLUGIN STYLES
    // BEGIN PAGE STYLES
    link(href= level + 'css/tasks.css', rel='stylesheet', type='text/css')
    // END PAGE STYLES
    // BEGIN THEME STYLES
    link#style_components(href='css/components.css', rel='stylesheet', type='text/css')
    link(href= level + 'css/plugins.css', rel='stylesheet', type='text/css')
    link(href= level + 'css/layout.css', rel='stylesheet', type='text/css')
    link#style_color(href= level + 'css/themes/darkblue.css', rel='stylesheet', type='text/css')
    link(href= level + 'css/custom.css', rel='stylesheet', type='text/css')
    // END THEME STYLES
    link(rel='shortcut icon', href= level + 'favicon.ico')